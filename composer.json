{
    "name": "andrey-helldar/laravel-lang-publisher",
    "description": "Publisher lang files for the Laravel Framework from caouecs/Laravel-lang",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "languages", "lpm", "lang", "publisher", "translations", "localization", "trans", "validations"],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/laravel-lang-publisher/issues",
        "source": "https://github.com/shahmaulik/laravel-lang-publisher"
    },
    "suggest": {
        "andrey-helldar/lang-translations": "Translation of main messages",
        "symfony/thanks": "Give thanks (in the form of a GitHub) to your fellow PHP package maintainers"
    },
    "require": {
        "php": "^7.1.3",
        "andrey-helldar/support": "^1.2",
        "illuminate/console": ">=5.5.0 <5.8.0",
        "illuminate/support": ">=5.5.0 <5.8.0",
        "laravel-lang/lang": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\LaravelLangPublisher\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\LaravelLangPublisher\\ServiceProvider"
            ]
        }
    }
}
